<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout
    android:layout_alignParentBottom="true"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:weightSum="5">

    <LinearLayout
        android:layout_weight="1"
        android:layout_centerVertical="true"
        android:layout_alignParentBottom="true"
        android:id="@+id/linear1"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:weightSum="6"
        android:orientation="vertical">

        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        </LinearLayout>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="MC"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
           />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="+/-"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="%"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="^"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="AC"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>

   <LinearLayout
       android:layout_weight="1"
    android:layout_centerVertical="true"
    android:layout_alignParentBottom="true"
       android:layout_toRightOf="@id/linear1"
    android:id="@+id/linear2"
    android:layout_width="0dp"
    android:layout_height="300dp"
    android:weightSum="6"
    android:orientation="vertical">
       <LinearLayout
           android:layout_weight="1"
           android:layout_width="match_parent"
           android:layout_height="0dp">

       </LinearLayout>
    <TextView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#e7d9d9"
        android:textAlignment="center"
        android:gravity="center"
        android:text="MR"
        android:textSize="30sp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        />
    <TextView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#e7d9d9"
        android:textAlignment="center"
        android:gravity="center"
        android:text="7"
        android:textSize="30sp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        />
    <TextView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#e7d9d9"
        android:textAlignment="center"
        android:gravity="center"
        android:text="4"
        android:textSize="30sp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        />
    <TextView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#e7d9d9"
        android:textAlignment="center"
        android:gravity="center"
        android:text="1"
        android:textSize="30sp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        />
    <TextView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#e7d9d9"
        android:textAlignment="center"
        android:gravity="center"
        android:text="0"
        android:textSize="30sp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        />
</LinearLayout>

    <LinearLayout
        android:layout_weight="1"
        android:layout_centerVertical="true"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/linear2"
        android:id="@+id/linear3"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:weightSum="6"
        android:orientation="vertical">
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        </LinearLayout>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="M-"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="8"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="5"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="2"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="."
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_weight="1"
        android:layout_centerVertical="true"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/linear3"
        android:id="@+id/linear4"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:weightSum="6"
        android:orientation="vertical">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="ON"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:background="#e7d9d9"
            android:gravity="center"
            android:text="M+"
            android:textAlignment="center"
            android:textSize="30sp" />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="9"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="6"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="3"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="="
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_weight="1"
        android:layout_centerVertical="true"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/linear4"
        android:id="@+id/linear5"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:weightSum="6"
        android:orientation="vertical">

        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="OFF"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="/"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="X"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="-"
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
        <TextView
            android:layout_weight="2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#e7d9d9"
            android:textAlignment="center"
            android:gravity="center"
            android:text="=="
            android:textSize="30sp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
</LinearLayout>

</RelativeLayout>